using System;

namespace fibonachi
{
    class Program
    {
        static long Fib(int n)
        {
            if(n==1 || n==2) { return 1; }
            else { return Fib(n - 1) + Fib(n - 2); }
        }
        static void Main(string[] args)
        {
            int f = int.Parse(Console.ReadLine());
            Console.WriteLine(Fib(f));
        }
    }
}
